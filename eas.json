{
  "cli": {
    "version": ">= 4.1.2"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EAS_USE_LEGACY_NPM": "true"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EAS_USE_LEGACY_NPM": "true"
      }
    },
    "preview3": {
      "developmentClient": true,
      "env": {
        "EAS_USE_LEGACY_NPM": "true"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EAS_USE_LEGACY_NPM": "true"
      }
    },
    "production": {
      "env": {
        "EAS_USE_LEGACY_NPM": "true"
      }
    }
  },

  "submit": {
    "production": {}
  }
}
